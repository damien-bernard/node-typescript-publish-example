{
  "name": "@damnard/node-typescript-publish-example-core",
  "version": "0.0.2",
  "description": "Exemple de publication d'application Node.js avec TypeScript",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./default": {
      "import": "./dist/default.js",
      "types": "./dist/default.d.ts"
    }
  },
  "scripts": {
    "clean": "tsc --build --clean",
    "build": "tsc --build",
    "fix": "eslint --fix src/**/*.ts && prettier .. --write",
    "lint": "eslint src/**/*.ts"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "keywords": [
    "node",
    "typescript",
    "core",
    "publish"
  ],
  "author": "Damien Bernard",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/damien-bernard/node-typescript-publish-example.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3"
  }
}
